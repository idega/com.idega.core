<project>
    <extend>../com.idega.core.bundle/base-project.xml</extend>
    <name>idegaWeb JavaServer Faces</name>
    <artifactId>com.idega.faces</artifactId>
    <currentVersion>1.0</currentVersion>
    <inceptionYear>2003</inceptionYear>
    <shortDescription>idegaWeb Faces is JavaServer Faces implementation</shortDescription>
    <versions/>
    <branches/>
    <mailingLists>
        <mailingList>
            <name>${pom.name} Dev List</name>
        </mailingList>
        <mailingList>
            <name>${pom.name} User List</name>
        </mailingList>
    </mailingLists>
    <developers>
        <developer>
            <id>tryggvil</id>
            <name>Tryggvi Larusson</name>
            <email>tryggvi@idega.is</email>
            <organization>idega software</organization>
            <roles/>
        </developer>
        <developer>
            <id>anders</id>
            <name>Anders Lindman</name>
            <email>anders.lindman@ncmedia.com</email>
            <organization></organization>
            <roles/>
        </developer>
    </developers>
    <dependencies>
        <dependency>
            <groupId>com.idega.core</groupId>
            <artifactId>com.idega.core</artifactId>
            <version>2.0</version>
            <type>jar</type>
            <url>http://www.idega.com/</url>
            <properties><eclipse.dependency>true</eclipse.dependency></properties>
        </dependency>
        <dependency>
            <groupId>jdom</groupId>
            <artifactId>jdom</artifactId>
            <version>b9-rc2</version>
            <type>jar</type>
            <url>http://www.jdom.org/</url>
            <properties/>
        </dependency>
        <dependency>
            <groupId>servletapi</groupId>
            <artifactId>servletapi</artifactId>
            <version>2.3</version>
            <type>jar</type>
            <url>http://java.sun.com/products/servlet/docs.html</url>
            <properties/>
        </dependency>
        <dependency>
            <groupId>ejb</groupId>
            <artifactId>ejb</artifactId>
            <version>2.0</version>
            <type>jar</type>
            <url>http://java.sun.com/products/ejb/docs.html</url>
            <properties/>
        </dependency>
        <dependency>
            <groupId>jta</groupId>
            <artifactId>jta</artifactId>
            <version>spec1_0_1</version>
            <type>jar</type>
            <url>http://java.sun.com/products/jta/jta-1_0_1B-doc</url>
            <properties/>
        </dependency>
        <dependency>
            <groupId>cos</groupId>
            <artifactId>cos</artifactId>
            <version>05Nov2002</version>
            <type>jar</type>
            <url>http://www.servlets.com/cos/</url>
            <properties/>
        </dependency>
        <dependency>
            <groupId>icu4j</groupId>
            <artifactId>icu4j</artifactId>
            <version>2.4</version>
            <type>jar</type>
            <url>http://oss.software.ibm.com/icu4j/</url>
            <properties/>
        </dependency>
        <dependency>
            <groupId>jsf</groupId>
            <artifactId>jsf-api</artifactId>
            <version>1.0</version>
            <type>jar</type>
            <url>http://java.sun.com/j2ee/javaserverfaces/</url>
            <properties/>
        </dependency>
    </dependencies>
    <build>
        <sourceDirectory>src/java</sourceDirectory>
        <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
        <unitTest>
            <includes>
                <include>**/*Test.java</include>
            </includes>
        </unitTest>
    </build>
    <reports>
        <report>maven-tasklist-plugin</report>
        <report>maven-changelog-plugin</report>
        <report>maven-developer-activity-plugin</report>
        <report>maven-simian-plugin</report>
        <report>maven-jcoverage-plugin</report>
        <report>maven-faq-plugin</report>
        <report>maven-file-activity-plugin</report>
        <report>maven-changes-plugin</report>
    </reports>
</project>
